<h1>Listing ticketsections</h1>

<table>
  <tr>
    <th>Showid</th>
    <th>Name</th>
    <th>Price (ID/no ID)</th>
    <th>Size</th>
  </tr>

<% @ticketsections.each do |ticketsection| %>
  <tr>
    <td><%=h ticketsection.showid %></td>
    <td><%=h ticketsection.name %></td>
    <td><%=h ticketsection.pricewithid %>/<%=h ticketsection.pricewoutid %></td>
    <td><%=h ticketsection.size %></td>
    <td><%= link_to 'Show', ticketsection %></td>
    <% if User.find(session[:user_id]).hasaccess("ticketsections", "update") %><td><%= link_to 'Edit', edit_ticketsection_path(ticketsection) %></td><% end %>
    <% if User.find(session[:user_id]).hasaccess("ticketsections", "destroy") %><td><%= link_to 'Destroy', ticketsection, :confirm => 'Are you sure?', :method => :delete %></td><% end %>
  </tr>
<% end %>
</table>

<br />

<% if User.find(session[:user_id]).hasaccess("ticketsections", "create") %><%= link_to 'New ticketsection', new_ticketsection_path+"/"+@show.abbrev %><% end %>
<br />
<br />

