<h1>Listing ticketsections</h1>

<table>
  <tr>
    <th>Showid</th>
    <th>Name</th>
    <th>Price (ID/no ID)</th>
    <th>Size</th>
  </tr>

<%
trclass = "class='zebra'"
@ticketsections.each do |ticketsection|
  if trclass=="class='zebra'"
    trclass = ""
  else
    trclass = "class='zebra'"
  end
%>
  <tr <%= trclass %>>
    <td><%=h ticketsection.showid %></td>
    <td><%=h ticketsection.name %></td>
    <td><%=h ticketsection.pricewithid %>/<%=h ticketsection.pricewoutid %></td>
    <td><%=h ticketsection.size %></td>
    <td><%= link_to 'Show', ticketsection %></td>
    <td><%= link_to 'Edit', edit_ticketsection_path(ticketsection) %></td>
    <% if session[:user_permissions]["ticketsections"]["d"] %><td><%= link_to 'Destroy', ticketsection, :confirm => 'Are you sure?', :method => :delete %></td><% end %>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New ticketsection', new_ticketsection_path+"/"+@show.abbrev %>
<br />
<br />

