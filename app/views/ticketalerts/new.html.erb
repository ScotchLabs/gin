<h1>New ticketalert</h1>

<% form_for(@ticketalert) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :showid %><br />
    <%= f.text_field :showid %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', shows_path %>