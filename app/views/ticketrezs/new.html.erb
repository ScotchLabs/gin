<h1>New ticketrez</h1>

<% form_for(@ticketrez) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :showid %><br />
    <%= f.text_field :showid %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :quantity %><br />
    <%= f.text_field :quantity %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :sectionid %><br />
    <%= f.text_field :sectionid %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

