<br />
<br />

<h1>Listing rezlineitems</h1>

<table>
  <tr>
    <th>Performance</th>
    <th>Section</th>
    <th>Quantity</th>
  </tr>

<%
trclass = "class='zebra'"
@rezlineitems.each do |rezlineitem|
  if trclass=="class='zebra'"
    trclass = ""
  else
    trclass = "class='zebra'"
  end
%>
  <tr <%= trclass %>>
    <td><%=h rezlineitem.performance %></td>
    <td><%=h rezlineitem.ticketsection %></td>
    <td><%=h rezlineitem.quantity %></td>
    <td><%= link_to 'Show', rezlineitem %></td>
    <td><%= link_to 'Edit', edit_rezlineitem_path(rezlineitem) %></td>
    <% if session[:user_permissions]["rezlineitems"]["d"] %><td><%= link_to 'Destroy', rezlineitem, :confirm => 'Are you sure?', :method => :delete %></td><% end %>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New rezlineitem', new_rezlineitem_path+"/"+@ticketrez.id.to_s %>