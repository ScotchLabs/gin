<h1>Editing roleassoc</h1>

<% form_for(@roleassoc) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :roleid %><br />
    <%= f.text_field :roleid %>
  </p>
  <p>
    <%= f.label :userid %><br />
    <%= f.text_field :userid %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @roleassoc %> |
<%= link_to 'Back', roleassocs_path %>